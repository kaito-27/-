package quiz;

public class Java67 {
	 public static void main(String[] args){

		 System.out.println(divide(2, 2));
		 System.out.println("例外");
	    }

	public static double divide(int a, int b){

		try {
			double ans = a / b;
			return ans;
		}
		catch(ArithmeticException e) {
			return 0;
		}
    }
}
