package quiz;

public class Java84 {
	public static void main(String[] args) {
		Employee e = new Employee("福田魁斗");
		String str = e.toString();

		System.out.println(str);
	}
}

class Employee {
	String name;

	public Employee(String name) {
		this.name = name;
	}

	public String toString() {
		return name;
	}

}
