package quiz;

public class Java86 {
	public static void main(String[] args) throws Exception {
        Child c = new Child();
        Parent p = (Parent)c;
        p.foo();

        Parent p2 = new Child();
        p2.foo();
    }
}

class Child extends Parent{
    public void foo(){
        System.out.println("ChildFoo");
    }
}

class Parent{
    public void foo(){
        System.out.println("ParentFoo");
    }
}